{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "pnpm run vercel-build",
  "installCommand": "pnpm install --frozen-lockfile --prefer-offline",
  "builds": [
    {
      "src": "apps/web/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/web/$1"
    }
  ]
}
