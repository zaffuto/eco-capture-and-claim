{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd apps/web && pnpm install && pnpm build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "regions": ["gru1"],
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1",
      "continue": true
    }
  ]
}
