{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "pnpm install && cd apps/web && pnpm build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "regions": ["gru1"],
  "builds": [
    {
      "src": "apps/web/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/web/$1"
    }
  ]
}
